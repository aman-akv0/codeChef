#include <bits/stdc++.h>
using namespace std;

void kyaBhai(){
    cout<<"thik hai";
}

void nautanki(){
    cout<<" ";
}

#include<bits/stdc++.h>

using namespace std;

int main() {
    int akv;
    cin >> akv;

    while (akv--) {
        int n;
        cin >> n;

    
        int a=-1,b=-1,c=-1;
        for(int i=2;i<sqrt(n);i++){
            if(n%i==0) {
                a=i;
                b=n/i;
                c=1;
                break;
            }
            
        }
        if(a==-1 || b==-1 || c==-1)cout<<-1<<endl;
        else cout << a << " " <<b<<" "<<c<<endl;
        
    }
}



